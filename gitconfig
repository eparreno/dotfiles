  tool = opendiff
[core]
  excludesfile = /Users/emili/.gitignore
  autocrlf = input
  safecrlf = true
  editor = mate -w
  quotepath = false
[alias]
  st = status
  ci = commit
  co = checkout
  br = branch
  up = !git fetch origin && git rebase origin/master
  out = log origin..HEAD
[color]
  ui = auto
[format]
  pretty = %Cblue%h%Creset %Cgreen[%ar]%Creset (%an) %s
